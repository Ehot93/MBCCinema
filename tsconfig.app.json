{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@/pages/*": [
        "./src/pages/*"
      ],
      "@/entities/*": [
        "./src/entities/*"
      ],
      "@/shared/*": [
        "./src/shared/*"
      ],
      "@/app/*": [
        "./src/app/*"
      ]
    }
  }
}